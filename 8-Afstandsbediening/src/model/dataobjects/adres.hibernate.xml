<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="model.dataobject.Adres" table="Address">  
    <id name="id">  
     <generator class="assigned"></generator>  
    </id>  
            
    <property name="street" column = "street" type = "string"></property>  
    <property name="number" column = "numbr" type = "int"></property>
    <property name="mailBox" column = "mailBox" type = "int"></property> 
    <property name="postalCode" column = "postalCode" type = "int"></property> 
    <property name="city" column = "city" type = "string"></property> 
    <property name="country" column = "country" type = "string"></property>   
            
  </class>  
            
 </hibernate-mapping>